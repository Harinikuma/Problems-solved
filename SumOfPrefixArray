class SumOfPrefixArray {
    public static void main(String[] args) {
        int[] arr = {3,5,7,9,11,2};
        int sum[] = new int[arr.length];
        int total=0;
        for(int i=0;i<arr.length;i++){
            total+=arr[i];
            sum[i]=total;
        }
        for(int i=0;i<arr.length;i++){
            System.out.println(sum[i]);
        }
    }
}
