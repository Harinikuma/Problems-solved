class 2DarraySum {
    public static void main(String[] args) {
        int arr1[][]={{3, 0, 1, 4, 2}, {5, 6, 3, 2, 1}, {1, 2, 0, 1, 5}, {4, 1, 0, 1, 7}, {1, 0, 3, 0, 5}};
        int arr2[][]={{2,1},{4,3}};
        int sum=0;
        for(int i=arr2[0][0];i<=arr2[1][0];i++){
            for(int j=arr2[0][1];j<=arr2[1][1];j++){
               sum+=arr1[i][j];
            }
        }
        System.out.println(sum);
    }
}
